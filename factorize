import sys

def factorize(n):
    factors = []
    d = 2
    while d * d <= n:
        while n % d == 0:
            factors.append(d)
            n //= d
        d += 1
    if n > 1:
        factors.append(n)
    return factors

def main():
    try:
        line = sys.stdin.read().strip()
        if not line:
            print("入力が空です", file=sys.stderr)
            sys.exit(1)
        x = int(line)
    except ValueError:
        print("整数を入力してください", file=sys.stderr)
        sys.exit(1)

    if x < 1:
        print("1以上の整数を入力してください", file=sys.stderr)
        sys.exit(1)

    if x == 1:
        print(1)
        sys.exit(0)

    factors = factorize(x)
    print("*".join(str(f) for f in factors))

main()


